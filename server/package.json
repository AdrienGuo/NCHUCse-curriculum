{
  "name": "react-express",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "client-install": "cd ../client/ && npm intall",
    "start": "npx babel-node ./lib/server.js",
    "server": "npm run build_backend && npm run start-build",
    "client": "cd ../client/ && npm start",
    "dev": "concurrently \"npm run server \" \"npm run client\"",
    "build_backend": "babel server -d lib",
    "build": "concurrently \"babel server -d lib \" \"cd ../client/ && npm run build\"",
    "start-build": "nodemon ./lib/server.js"
  },
  "author": "mushding",
  "license": "ISC",
  "dependencies": {
    "@types/mysql": "^2.15.15",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "nodemon": "^2.0.4"
  },
  "proxy": "http://localhost:5000"
}
